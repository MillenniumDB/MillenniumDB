{
    "cmake.configureOnOpen": true,
    "C_Cpp.dimInactiveRegions": true,
    "clangd.arguments": [
        "--function-arg-placeholders=0",
        "--header-insertion=never"
    ],
    "[restructuredtext]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 3,
    },
    "python.analysis.typeCheckingMode": "strict",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        }
    },
    "isort.args": [
        "--profile=black"
    ],
    "black-formatter.args": [
        "--line-length=120"
    ],
    "pylint.args": [
        "--max-line-length=120",
        "--disable=R0902", // too-many-instance-attributes
        "--disable=R0903", // too-few-public-methods
        "--disable=R0912", // too-many-branches
        "--disable=R0914", // too-many-locals
        "--disable=R0915", // too-many-statements
        "--disable=R1732", // consider-using-with
        "--disable=C0103", // invalid-name
        "--disable=C0114", // missing-module-docstring
        "--disable=C0115", // missing-class-docstring
        "--disable=C0116", // missing-function-docstring
        "--disable=W0603", // global-statement
        "--disable=W0621", // redefined-outer-name
        "--disable=W0718", // broad-exception-caught
        "--disable=W0719", // broad-exception-raised
        "--disable=W1510", // subprocess-run-check
    ],
    "todohighlight.isEnable": true,
    "cSpell.enabled": true,
    "cSpell.words": [
        "antlr",
        "antlrcpp",
