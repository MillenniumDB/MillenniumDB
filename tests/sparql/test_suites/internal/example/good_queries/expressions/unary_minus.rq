PREFIX : <http://www.firstPrefix.com/>
SELECT *
WHERE {
    :subjectFirstPrefix :predicateFirstPrefix ?o .
    FILTER (true
         && "20"^^xsd:nonPositiveInteger = -("-20"^^xsd:nonPositiveInteger)
         && 10 = -(-10)
         && 4 = -(-4)
         && "40"^^xsd:unsignedShort = -("-40"^^xsd:unsignedShort)
         && 7.5e5 = -(-7.5e5)
         && ".100"^^xsd:decimal = -("-.100"^^xsd:decimal)
         && "-0.0"^^xsd:decimal = -("-0.0"^^xsd:decimal)
         && "+100"^^xsd:decimal = -(-("+100"^^xsd:decimal))
         && "123456789123456789123456789123456789.123456789123456789123456789123456789"^^xsd:decimal = -("-123456789123456789123456789123456789.123456789123456789123456789123456789"^^xsd:decimal)
         && "0.123456789123456789123456789123456789123456789123456789123456789123456789"^^xsd:decimal = -("-0.123456789123456789123456789123456789123456789123456789123456789123456789"^^xsd:decimal)
         && "-123456789123456789123456789123456789123456789123456789123456789123456789"^^xsd:decimal = -("123456789123456789123456789123456789123456789123456789123456789123456789"^^xsd:decimal)
         && "-123456789123456789123456789123456789123456789123456789123456789123456789"^^xsd:decimal = -(-(-("123456789123456789123456789123456789123456789123456789123456789123456789"^^xsd:decimal)))
    )
}
